{
    "name": "printer-mock",
    "version": "1.0.0",
    "description": "printer mock script",
    "main": "dist/index.js",
    "scripts": {
        "start": "nodemon src/index.ts",
        "start:node2": "nodemon --config nodemon.node2.json src/index.ts",
        "start:prod": "cross-env NODE_ENV=production node dist/index.js",
        "clear": "find ./logs -type f -iname '*.txt' -delete",
        "test": "jest --no-cache"
    },
    "keywords": [
        "nodejs",
        "mock"
    ],
    "author": "PedroZC90 <pedrozc90@gmail.com>",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "@types/node": "^22.13.8",
        "cross-env": "^7.0.3",
        "jest": "^29.6.2",
        "nodemon": "^3.1.9",
        "ts-jest": "^29.2.6",
        "ts-node": "^10.9.1",
        "typescript": "^5.8.2"
    },
    "engines": {
        "node": ">=20.10.0",
        "npm": ">=10.2.3"
    }
}
